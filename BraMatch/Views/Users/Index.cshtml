@model IEnumerable<EntityDataAccess.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisplayName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Region)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MeasureSystem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BandMeasure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WidestMeasure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PointMeasure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Implants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reduction)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisplayName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Region)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MeasureSystem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BandMeasure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WidestMeasure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PointMeasure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Implants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reduction)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
